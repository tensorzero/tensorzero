{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "Base64FileMetadata",
  "description": "Like `Base64File`, but without the data field.",
  "type": "object",
  "properties": {
    "source_url": {
      "type": [
        "string",
        "null"
      ]
    },
    "mime_type": {
      "type": "string"
    },
    "detail": {
      "anyOf": [
        {
          "$ref": "#/$defs/Detail"
        },
        {
          "type": "null"
        }
      ]
    }
  },
  "required": [
    "mime_type"
  ],
  "$defs": {
    "Detail": {
      "description": "Detail level for input images (affects fidelity and token cost)",
      "type": "string",
      "enum": [
        "low",
        "high",
        "auto"
      ]
    }
  }
}