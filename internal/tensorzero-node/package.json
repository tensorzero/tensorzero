{
  "name": "tensorzero-node",
  "version": "0.0.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "napi": {
    "name": "tensorzero-node",
    "triples": {
      "defaults": false,
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "x86_64-apple-darwin",
        "x86_64-unknown-linux-gnu",
        "x86_64-unknown-linux-musl"
      ]
    }
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@napi-rs/cli": "^2.18.4",
    "vitest": "^1.0.0",
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0"
  },
  "vitest": {
    "testTimeout": 180000
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release && tsc",
    "build:debug": "napi build --platform && tsc",
    "build:ts": "tsc",
    "check-exports": "cd lib/bindings && node check-exports.js",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "vitest",
    "universal": "napi universal",
    "version": "napi version"
  }
}
