{
  "name": "tensorzero-node",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "napi": {
    "binaryName": "tensorzero-node",
    "targets": [
      "aarch64-apple-darwin",
      "aarch64-unknown-linux-gnu",
      "aarch64-unknown-linux-musl",
      "x86_64-apple-darwin",
      "x86_64-unknown-linux-gnu",
      "x86_64-unknown-linux-musl"
    ]
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@napi-rs/cli": "^3.0.0-alpha.72",
    "@types/node": "^20.19.4",
    "vitest": "^4.0.8"
  },
  "vitest": {
    "testTimeout": 180000
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build-bindings": "./build-bindings.sh",
    "build": "napi build --platform --release --js index.cjs && tsc",
    "build:debug": "napi build --platform --js index.cjs && tsc",
    "build:ts": "tsc",
    "check-exports": "cd lib && node check-exports.js",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "vitest",
    "typecheck": "tsc",
    "format": "biome format --write",
    "format:check": "biome format",
    "lint": "biome check --write",
    "lint:check": "biome check",
    "universal": "napi universalize",
    "version": "napi version"
  },
  "dependencies": {
    "buildkite-test-collector": "^1.9.1"
  }
}
