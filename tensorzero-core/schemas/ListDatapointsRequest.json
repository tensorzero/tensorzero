{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "ListDatapointsRequest",
  "description": "Request to list datapoints from a dataset with pagination and filters.\nUsed by the `POST /v1/datasets/{dataset_id}/list_datapoints` endpoint.",
  "type": "object",
  "properties": {
    "function_name": {
      "description": "Optional function name to filter datapoints by.\nIf provided, only datapoints from this function will be returned.",
      "type": [
        "string",
        "null"
      ]
    },
    "limit": {
      "description": "The maximum number of datapoints to return.\nDefaults to 20.",
      "type": [
        "integer",
        "null"
      ],
      "format": "uint32",
      "minimum": 0
    },
    "page_size": {
      "description": "The maximum number of datapoints to return. Defaults to 20.\nDeprecated: please use `limit`. If `limit` is provided, `page_size` is ignored.",
      "type": [
        "integer",
        "null"
      ],
      "format": "uint32",
      "minimum": 0,
      "deprecated": true
    },
    "offset": {
      "description": "The number of datapoints to skip before starting to return results.\nDefaults to 0.",
      "type": [
        "integer",
        "null"
      ],
      "format": "uint32",
      "minimum": 0
    },
    "filter": {
      "description": "Optional filter to apply when querying datapoints.\nSupports filtering by tags, time, and logical combinations (AND/OR/NOT).",
      "anyOf": [
        {
          "$ref": "#/$defs/DatapointFilter"
        },
        {
          "type": "null"
        }
      ]
    }
  },
  "$defs": {
    "DatapointFilter": {
      "description": "Filter tree for querying datapoints.\nThis is similar to `InferenceFilter` but without metric filters, as datapoints don't have associated metrics.",
      "oneOf": [
        {
          "title": "TagDatapointFilter",
          "description": "Filter by tag key-value pair",
          "type": "object",
          "properties": {
            "type": {
              "type": "string",
              "const": "tag"
            }
          },
          "required": [
            "type"
          ],
          "allOf": [
            {
              "$ref": "#/$defs/TagFilter"
            }
          ]
        },
        {
          "title": "TimeDatapointFilter",
          "description": "Filter by datapoint update time",
          "type": "object",
          "properties": {
            "type": {
              "type": "string",
              "const": "time"
            }
          },
          "required": [
            "type"
          ],
          "allOf": [
            {
              "$ref": "#/$defs/TimeFilter"
            }
          ]
        },
        {
          "title": "AndDatapointFilter",
          "description": "Logical AND of multiple filters",
          "type": "object",
          "properties": {
            "children": {
              "type": "array",
              "items": {
                "$ref": "#/$defs/DatapointFilter"
              }
            },
            "type": {
              "type": "string",
              "const": "and"
            }
          },
          "required": [
            "type",
            "children"
          ]
        },
        {
          "title": "OrDatapointFilter",
          "description": "Logical OR of multiple filters",
          "type": "object",
          "properties": {
            "children": {
              "type": "array",
              "items": {
                "$ref": "#/$defs/DatapointFilter"
              }
            },
            "type": {
              "type": "string",
              "const": "or"
            }
          },
          "required": [
            "type",
            "children"
          ]
        },
        {
          "title": "NotDatapointFilter",
          "description": "Logical NOT of a filter",
          "type": "object",
          "properties": {
            "child": {
              "$ref": "#/$defs/DatapointFilter"
            },
            "type": {
              "type": "string",
              "const": "not"
            }
          },
          "required": [
            "type",
            "child"
          ]
        }
      ]
    },
    "TagComparisonOperator": {
      "description": "Comparison operators for tag filters.",
      "type": "string",
      "enum": [
        "=",
        "!="
      ]
    },
    "TagFilter": {
      "description": "Filter by tag key-value pair.",
      "type": "object",
      "properties": {
        "key": {
          "type": "string"
        },
        "value": {
          "type": "string"
        },
        "comparison_operator": {
          "$ref": "#/$defs/TagComparisonOperator"
        }
      },
      "required": [
        "key",
        "value",
        "comparison_operator"
      ]
    },
    "TimeComparisonOperator": {
      "description": "Comparison operators for timestamps.",
      "type": "string",
      "enum": [
        "<",
        "<=",
        "=",
        ">",
        ">=",
        "!="
      ]
    },
    "TimeFilter": {
      "description": "Filter by timestamp.",
      "type": "object",
      "properties": {
        "time": {
          "type": "string"
        },
        "comparison_operator": {
          "$ref": "#/$defs/TimeComparisonOperator"
        }
      },
      "required": [
        "time",
        "comparison_operator"
      ]
    }
  }
}
