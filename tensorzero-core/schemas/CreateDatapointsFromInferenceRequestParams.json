{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "CreateDatapointsFromInferenceRequestParams",
  "description": "Parameters for creating datapoints from inferences.\nCan specify either a list of inference IDs or a query to find inferences.",
  "oneOf": [
    {
      "title": "CreateDatapointsFromInferenceRequestParamsInferenceIds",
      "description": "Create datapoints from specific inference IDs.",
      "type": "object",
      "properties": {
        "inference_ids": {
          "description": "The inference IDs to create datapoints from.",
          "type": "array",
          "items": {
            "type": "string",
            "format": "uuid"
          }
        },
        "type": {
          "type": "string",
          "const": "inference_ids"
        }
      },
      "required": [
        "type",
        "inference_ids"
      ]
    },
    {
      "title": "CreateDatapointsFromInferenceRequestParamsInferenceQuery",
      "description": "Create datapoints from an inference query.",
      "type": "object",
      "properties": {
        "function_name": {
          "description": "The function name to filter inferences by.",
          "type": "string"
        },
        "variant_name": {
          "description": "Variant name to filter inferences by, optional.",
          "type": [
            "string",
            "null"
          ]
        },
        "filters": {
          "description": "Filters to apply when querying inferences, optional.",
          "anyOf": [
            {
              "$ref": "#/$defs/InferenceFilter"
            },
            {
              "type": "null"
            }
          ]
        },
        "type": {
          "type": "string",
          "const": "inference_query"
        }
      },
      "required": [
        "type",
        "function_name"
      ]
    }
  ],
  "$defs": {
    "InferenceFilter": {
      "description": "Filters for querying inferences.",
      "oneOf": [
        {
          "title": "InferenceFilterFloatMetric",
          "description": "Filter by the value of a float metric",
          "type": "object",
          "properties": {
            "type": {
              "type": "string",
              "const": "float_metric"
            }
          },
          "required": [
            "type"
          ],
          "allOf": [
            {
              "$ref": "#/$defs/FloatMetricFilter"
            }
          ]
        },
        {
          "title": "InferenceFilterBooleanMetric",
          "description": "Filter by the value of a boolean metric",
          "type": "object",
          "properties": {
            "type": {
              "type": "string",
              "const": "boolean_metric"
            }
          },
          "required": [
            "type"
          ],
          "allOf": [
            {
              "$ref": "#/$defs/BooleanMetricFilter"
            }
          ]
        },
        {
          "title": "InferenceFilterTag",
          "description": "Filter by tag key-value pair",
          "type": "object",
          "properties": {
            "type": {
              "type": "string",
              "const": "tag"
            }
          },
          "required": [
            "type"
          ],
          "allOf": [
            {
              "$ref": "#/$defs/TagFilter"
            }
          ]
        },
        {
          "title": "InferenceFilterTime",
          "description": "Filter by the timestamp of an inference.",
          "type": "object",
          "properties": {
            "type": {
              "type": "string",
              "const": "time"
            }
          },
          "required": [
            "type"
          ],
          "allOf": [
            {
              "$ref": "#/$defs/TimeFilter"
            }
          ]
        },
        {
          "title": "InferenceFilterAnd",
          "description": "Logical AND of multiple filters",
          "type": "object",
          "properties": {
            "children": {
              "type": "array",
              "items": {
                "$ref": "#/$defs/InferenceFilter"
              }
            },
            "type": {
              "type": "string",
              "const": "and"
            }
          },
          "required": [
            "type",
            "children"
          ]
        },
        {
          "title": "InferenceFilterOr",
          "description": "Logical OR of multiple filters",
          "type": "object",
          "properties": {
            "children": {
              "type": "array",
              "items": {
                "$ref": "#/$defs/InferenceFilter"
              }
            },
            "type": {
              "type": "string",
              "const": "or"
            }
          },
          "required": [
            "type",
            "children"
          ]
        },
        {
          "title": "InferenceFilterNot",
          "description": "Logical NOT of a filter",
          "type": "object",
          "properties": {
            "child": {
              "$ref": "#/$defs/InferenceFilter"
            },
            "type": {
              "type": "string",
              "const": "not"
            }
          },
          "required": [
            "type",
            "child"
          ]
        }
      ]
    },
    "FloatComparisonOperator": {
      "description": "Comparison operators for float metrics.",
      "type": "string",
      "enum": [
        "<",
        "<=",
        "=",
        ">",
        ">=",
        "!="
      ]
    },
    "FloatMetricFilter": {
      "type": "object",
      "properties": {
        "metric_name": {
          "type": "string"
        },
        "value": {
          "type": "number",
          "format": "double"
        },
        "comparison_operator": {
          "$ref": "#/$defs/FloatComparisonOperator"
        }
      },
      "required": [
        "metric_name",
        "value",
        "comparison_operator"
      ]
    },
    "BooleanMetricFilter": {
      "type": "object",
      "properties": {
        "metric_name": {
          "type": "string"
        },
        "value": {
          "type": "boolean"
        }
      },
      "required": [
        "metric_name",
        "value"
      ]
    },
    "TagComparisonOperator": {
      "description": "Comparison operators for tag filters.",
      "type": "string",
      "enum": [
        "=",
        "!="
      ]
    },
    "TagFilter": {
      "description": "Filter by tag key-value pair.",
      "type": "object",
      "properties": {
        "key": {
          "type": "string"
        },
        "value": {
          "type": "string"
        },
        "comparison_operator": {
          "$ref": "#/$defs/TagComparisonOperator"
        }
      },
      "required": [
        "key",
        "value",
        "comparison_operator"
      ]
    },
    "TimeComparisonOperator": {
      "description": "Comparison operators for timestamps.",
      "type": "string",
      "enum": [
        "<",
        "<=",
        "=",
        ">",
        ">=",
        "!="
      ]
    },
    "TimeFilter": {
      "description": "Filter by timestamp.",
      "type": "object",
      "properties": {
        "time": {
          "type": "string"
        },
        "comparison_operator": {
          "$ref": "#/$defs/TimeComparisonOperator"
        }
      },
      "required": [
        "time",
        "comparison_operator"
      ]
    }
  }
}
